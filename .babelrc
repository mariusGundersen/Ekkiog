{
  "presets": [
    ["env", {
      "spec": true,
      "modules": false,
      "targets" : {
        "ios": 10,
        "chrome": 58,
        "firefox": 53
      }
    }],
    "es2016",
    "es2017",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }],
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}