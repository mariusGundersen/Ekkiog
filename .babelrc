{
  "presets": [
    ["es2015", {"spec": true, "modules": false}],
    "es2016",
    "es2017",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }],
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}