{
  "presets": [
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": true,
      "regenerator": true
    }],
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre",
        ["env", {
          "targets": {
            "chrome": 54
          }
        }]
      ]
    },
    "production": {
      "presets": [
        "es2015",
        "es2016",
        "es2017"
      ]
    }
  }
}